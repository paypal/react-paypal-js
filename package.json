{
    "name": "@paypal/react-paypal-js",
    "version": "7.4.2",
    "description": "React components for the PayPal JS SDK",
    "keywords": [
        "react",
        "component",
        "paypal",
        "button",
        "checkout",
        "payment",
        "paypal javascript sdk",
        "paypal smart buttons"
    ],
    "main": "index.js",
    "module": "dist/esm/react-paypal-js.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "build": "rollup --config",
        "check-node-version": "node scripts/check-node-version.js",
        "format": "prettier --write .",
        "lint": "eslint .",
        "postinstall": "husky install",
        "prepack": "rimraf dist && npm run build && npm run type-declarations",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "test": "jest --env=jsdom",
        "start": "npm run check-node-version && npm run storybook",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "deploy-storybook": "storybook-to-ghpages",
        "prerelease": "npm run validate",
        "release": "standard-version",
        "postrelease": "git push && git push --follow-tags && npm run build && npm publish",
        "typecheck": "tsc --noEmit",
        "type-declarations": "tsc --emitDeclarationOnly --outDir dist/types --project tsconfig.declarations.json",
        "validate": "npm run check-node-version && npm run typecheck && npm run build && npm run lint && npm test -- --coverage"
    },
    "files": [
        "dist"
    ],
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git://github.com/paypal/react-paypal-js.git"
    },
    "homepage": "https://paypal.github.io/react-paypal-js/",
    "dependencies": {
        "@paypal/paypal-js": "^4.2.1",
        "@paypal/sdk-constants": "^1.0.110"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@codesandbox/sandpack-react": "^0.1.15",
        "@commitlint/cli": "^13.2.0",
        "@commitlint/config-conventional": "^13.2.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-node-resolve": "^13.0.5",
        "@rollup/plugin-replace": "^3.0.0",
        "@rollup/plugin-typescript": "^8.2.5",
        "@storybook/addon-actions": "^6.3.8",
        "@storybook/addon-docs": "^6.3.12",
        "@storybook/addon-essentials": "^6.3.8",
        "@storybook/addon-links": "^6.3.8",
        "@storybook/react": "^6.3.8",
        "@storybook/storybook-deployer": "^2.8.10",
        "@testing-library/react": "^12.1.1",
        "@types/jest": "^27.0.2",
        "@types/react": "^17.0.25",
        "@types/react-dom": "^17.0.9",
        "@types/string-template": "^1.0.2",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "babel-jest": "^27.2.4",
        "babel-loader": "^8.2.2",
        "chalk": "^4.1.2",
        "check-node-version": "^4.1.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.26.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^7.0.2",
        "jest": "^27.2.4",
        "jest-mock-extended": "^2.0.4",
        "lint-staged": "^11.1.2",
        "pinst": "^2.1.6",
        "prettier": "^2.4.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-element-to-jsx-string": "^14.3.4",
        "react-error-boundary": "^3.1.3",
        "react-is": "^17.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.57.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-terser": "^7.0.2",
        "shelljs": "^0.8.4",
        "standard-version": "^9.3.1",
        "string-template": "^1.0.0",
        "typescript": "^4.4.3"
    },
    "peerDependencies": {
        "react": ">=16.3.0",
        "react-dom": ">=16.3.0"
    },
    "jest": {
        "transformIgnorePatterns": [
            "/!node_modules\\/@paypal\\/sdk-constants/"
        ]
    }
}
